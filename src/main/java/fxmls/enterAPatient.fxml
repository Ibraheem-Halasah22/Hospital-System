<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EnterAPatient">
   <children>
      <RadioButton fx:id="emerRaButton" layoutX="462.0" layoutY="67.0" mnemonicParsing="false" text="Emergency">
         <toggleGroup>
            <ToggleGroup fx:id="Type" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="depRaButton" layoutX="459.0" layoutY="123.0" mnemonicParsing="false" text="Department" toggleGroup="$Type" />
      <ChoiceBox fx:id="Department" layoutX="108.0" layoutY="132.0" prefWidth="150.0" />
      <Button layoutX="288.0" layoutY="281.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <ChoiceBox fx:id="Patient" layoutX="108.0" layoutY="72.0" prefWidth="150.0" />
      <Hyperlink layoutX="49.0" layoutY="307.0" onAction="#goBack" text="Return" />
      <Label fx:id="patLabel" layoutX="300.0" layoutY="76.0" prefHeight="18.0" prefWidth="110.0" text="Patient" />
      <Label fx:id="departLabel" layoutX="300.0" layoutY="131.0" prefHeight="18.0" prefWidth="110.0" text="Department" />
   </children>
</AnchorPane>
