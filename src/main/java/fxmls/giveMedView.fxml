<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="104.0" prefWidth="528.0" stylesheets="@addpatient.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GiveMedView">
   <children>
      <ImageView fitHeight="396.0" fitWidth="656.0" opacity="0.7" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/unnamed.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="patField" editable="false" layoutX="100.0" layoutY="53.0" prefHeight="26.0" prefWidth="182.0" promptText="Choose Patient" />
      <TextField fx:id="medecineField" editable="false" layoutX="376.0" layoutY="53.0" prefHeight="26.0" prefWidth="182.0" promptText="Choose medecine" />
      <TextField fx:id="dozenField" layoutX="100.0" layoutY="292.0" prefHeight="26.0" prefWidth="86.0" promptText="Dozen" />
      <TextField fx:id="perDayField" layoutX="288.0" layoutY="292.0" prefHeight="26.0" prefWidth="106.0" promptText="Dozens\Day" />
      <TextField fx:id="daysField" layoutX="511.0" layoutY="292.0" prefHeight="26.0" prefWidth="94.0" promptText="NO. of Days" />
      <Button layoutX="315.0" layoutY="356.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <ChoiceBox fx:id="patSelect" layoutX="100.0" layoutY="116.0" prefHeight="26.0" prefWidth="182.0" />
      <ChoiceBox fx:id="medecineSelect" layoutX="376.0" layoutY="116.0" prefHeight="26.0" prefWidth="182.0" />
      <Hyperlink layoutX="41.0" layoutY="333.0" onAction="#goBack" text="Return" />
      <Hyperlink layoutX="500.0" layoutY="344.0" onAction="#goToAddNewMed" text="New Medecine" />
   </children>
</AnchorPane>
