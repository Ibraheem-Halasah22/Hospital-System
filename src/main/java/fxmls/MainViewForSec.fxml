<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="172.0" prefWidth="758.0" stylesheets="@mainviewforsecrt.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainViewForSec">
   <children>
      <ImageView fitHeight="384.0" fitWidth="832.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/pngtree-modern-double-color-futuristic-neon-background-image_351866.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="5.0" layoutY="14.0" prefHeight="94.0" prefWidth="435.0">
         <children>
            <Label fx:id="nameLabel" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="175.0" text="Name:" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="idLabel" layoutX="14.0" layoutY="47.0" prefHeight="26.0" prefWidth="175.0" text="ID:" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="515.0" layoutY="221.0" mnemonicParsing="false" onAction="#goToEnterPat" prefHeight="23.0" prefWidth="152.0" text="Enter Patient" />
      <Button layoutX="168.0" layoutY="195.0" mnemonicParsing="false" onAction="#goToAllDocs" prefHeight="26.0" prefWidth="155.0" text="View Whole Doctors" />
      <Button layoutX="168.0" layoutY="134.0" mnemonicParsing="false" onAction="#goToAllPats" prefHeight="26.0" prefWidth="155.0" text="View Whole Patients" />
      <Button layoutX="168.0" layoutY="249.0" mnemonicParsing="false" onAction="#goToAllNurs" prefHeight="26.0" prefWidth="155.0" text="View Whole Nurses" />
      <Button layoutX="681.0" layoutY="343.0" mnemonicParsing="false" onAction="#logOut" prefHeight="26.0" prefWidth="118.0" text="Log Out" />
      <Button layoutX="515.0" layoutY="262.0" mnemonicParsing="false" onAction="#goToExitPat" prefHeight="23.0" prefWidth="152.0" text="Exit Patient" />
      <Button layoutX="515.0" layoutY="148.0" mnemonicParsing="false" onAction="#goToAddPat" prefHeight="23.0" prefWidth="152.0" text="Register Patient" />
      <Button layoutX="515.0" layoutY="108.0" mnemonicParsing="false" onAction="#goToAddDoc" text="Register Doctor Or Nurse" />
   </children>
</AnchorPane>
